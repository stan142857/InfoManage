                            <tr>
                                <td class="auto-style6">
                        <asp:Image ID="ImgInfo" runat="server" Height="46px" Width="46px" ImageUrl="~/Img/毕设.png" />
                                </td>
                                <td>
                                    <asp:ContentPlaceHolder ID="ContentPlaceHolder2" runat="server">
                                    </asp:ContentPlaceHolder>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td class="auto-style4" style="background-image: url('/Img/backgroundAll.jpg')">
                        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                        </asp:ContentPlaceHolder>
                    </td>
                </tr>
            </table>
        </div>
    </form>
</body>
</html>
